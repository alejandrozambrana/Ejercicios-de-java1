 */

public class Ejercicio7 { // Clase principal
  public static void main(String[] args) {
    System.out.println("    *");
    System.out.println("   * *");
    System.out.println("  *   *");
    System.out.println(" *     *");
    System.out.println("*********");
   }
 }
